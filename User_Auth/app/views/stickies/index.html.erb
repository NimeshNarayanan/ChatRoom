<p id="notice"><%= notice %></p>
<%= link_to 'New sticky', controller: 'stickies', action: 'rout' %>
<h1>Listing Stickies</h1>

<div class= "sticky_container">
  <% @stickies.each do |sticky| %>
  <div class="sticky">
    <div class="sticky_title">
      <%= sticky.title %>
    </div>
    <div class="sticky_content">
      <%= sticky.content %>
    </div>
  </div>
  <% end %>
</div>

<br>

